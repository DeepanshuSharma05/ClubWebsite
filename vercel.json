{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "framework": "vite",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
 "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
